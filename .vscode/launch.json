        {
            "name": "myhome ctl shelly jobs show <IP>",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/myhome",
            "args": ["ctl", "-v", "-B", "192.168.1.2", "shelly", "-H", "jobs", "show", "192.168.1.66"],
            "env": {
                "MYHOME_LOG": "stderr"
            }
        },
        {
