    "version": "0.2.0",
    "configurations": [

        {
            "name": "myhome ctl shelly jobs show <IP>",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/myhome",
            "args": [
                "ctl",
                "--debug",
                "-B", "192.168.1.2",
                "shelly", "-H", "jobs", "show", "192.168.1.66"
            ],
            "env": {
                "MYHOME_LOG": "stderr"
            }
        },
        {
            "name": "myhome ctl shelly jobs show <ID>",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/myhome",
            "args": [
                "ctl",
                "--debug",
                "-v",
                "-B", "192.168.1.2",
                "shelly", "jobs", "show", "shellyplus1-08b61fd9d708"
            ],
            "env": {
                "MYHOME_LOG": "stderr"
            }
        },
